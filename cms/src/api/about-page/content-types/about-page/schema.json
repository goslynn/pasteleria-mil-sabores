{
  "kind": "singleType",
  "collectionName": "about_pages",
  "info": {
    "singularName": "about-page",
    "pluralName": "about-pages",
    "displayName": "AboutPage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "header": {
      "type": "string",
      "default": "Acerca de Nosotros",
      "required": true
    },
    "background": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "about_sections": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article-section.article-section"
    }
  }
}
